{
  "routes": [
    {
      "src": "/gh/(.*)",
      "dest": "https://cdn.jsdelivr.net/gh/$1"
    }
  ]
  "routes": [
    {
      "src": "/npm/(.*)",
      "dest": "https://cdn.jsdelivr.net/npm/$1"
    }
  ]
  "routes": [
    {
      "src": "/wp/(.*)",
      "dest": "https://cdn.jsdelivr.net/wp/$1"
    }
  ]
  "routes": [
    {
      "src": "/purge/(.*)",
      "dest": "https://purge.jsdelivr.net/$1"
    }
  ]
  "routes": [
    {
      "src": "/avatar/(.*)",
      "dest": "https://cn.gravatar.com/$1"
    }
  ]
}
